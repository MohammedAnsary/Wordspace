  <div class="editor" >
    <h1>My Magazines</h1>

       <div class="row">

       </div>

        <% @magazines.each do |magazine| %>

          <div class="row">
          <div class="col-sm-3 ">
          <h4><%= magazine.name %></h4>
          </div>

          <div class="col-sm-3 ">
          <h4>  <%= link_to 'Show', magazine, :class => 'btn btn-outlined btn-grey'%> </h4>

          </div>
           </div>
       <%end%>
      <% if @magazines.size == 0 %>
        <em>You are currently not managing any magazine</em>
      <% end %>


      </div>
